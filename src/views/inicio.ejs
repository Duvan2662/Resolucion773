<body class="body">
    <header class="container-fluid d-flex align-items-center py-4">
        <div class="logo">
            <a id="logo-header" href="https://www.udistrital.edu.co/inicio">
                <img src="/img/logo.png" alt="Logo Universidad" width="300" height="auto">
            </a>
        </div>
        <div class="titulo">
            <h1>Resolución 773 de la ANE</h1>
        </div>
    </header>

    <div class="container mt-5">
        <form action="/resultados" method="post" class="row g-3">
            <div class="col-md-6 mb-3">
                <label for="inputState" class="form-label fs-3  text-white">Servicio</label>
                <select id="servicio" class="form-select" name="servicio" required>
                    <option selected>Seleccione un servicio</option>
                    <option>Radiodifusión</option>
                    <!-- <option>Móvil</option>
                    <option>Fijo</option> -->
                </select>
            </div>
            
            <div class="col-md-6 mb-3" id="referecias">
                
            </div>


            <div class="col-md-6 mb-3">
                <label for="inputState" class="form-label fs-3  text-white">Ubicación de la antena</label>
                <select id="ubicacion" class="form-select" name="ubicacion" required>
                    <option selected>Selecciona un tipo</option>
                    <option>Acceso público</option>
                    <option>Acceso restringido</option>
                </select>
            </div>

            <div class="col-md-6 mb-3">
                <label for="potencia" class="form-label fs-3  text-white">Potencia transmitida</label>
                <div class="input-group">
                    <input type="number" class="form-control" id="potencia" name="potencia" required>
                    <span class="input-group-text fw-bold">W</span>
                </div>
            </div>

            
            <div class="col-md-6 mb-3">
                <label for="frecuencia" class="form-label fs-3  text-white">Frecuencia de la antena</label>
                <div class="input-group">
                    <input type="number" class="form-control" id="frecuencia" name="frecuencia" required>
                    <span class="input-group-text fw-bold">MHz</span>
                </div>
            </div>
            <div class="col-md-6 mb-3">
                <label for="altura" class="form-label fs-3  text-white">Altura de la antena</label>
                <div class="input-group">
                    <input type="number" class="form-control" id="altura" name="altura" required>
                    <span class="input-group-text fw-bold">m</span>
                </div>
            </div>
            <div class="col-md-6 mb-3">
                <label for="ganancia" class="form-label fs-3  text-white">Ganancia de la antena</label>
                <div class="input-group">
                    <input type="number" class="form-control" id="ganancia" name="ganancia" required>
                    <span class="input-group-text fw-bold">dBi</span>
                </div>
            </div>

            <div class="col-md-6 mb-3">
                <label for="inputState" class="form-label fs-3  text-white">Hay otra antena en un radio de 100 m?</label>
                <select id="pregunta" class="form-select" name="pregunta" required>
                    <option selected>No</option>
                    <option>Si</option>
                </select>
            </div>

            <div class="col-12 d-grid">
                <button type="submit" class="btn btn-light fs-3 ">Verificar normatividad</button>
            </div>

        </form>
    </div>

    <!-- Footer -->
<footer class="footer fixed-bottom py-3 ">
    <div id="footer" class="container-fluid text-center">
        <div class="row">
            <div class="col-md-4 text-white">
                <a class="text-white text-decoration-none" id="logo-header" href="http://www1.udistrital.edu.co:8080/web/facultad-tecnologica/inicio">
                    <h5>FACULTAD TECNOLÓGICA</h5>
                </a>
                <ul class="list-unstyled">
                    <li >Proyecto</li>
                    <a class="text-white text-decoration-none" id="logo-header" href="http://tecsistematizaciondatos.udistrital.edu.co:8080/inicio">
                        <li>Sistematizacion de datos</li>
                    </a> 
                </ul>
                <li >Abril-2024</li> 
            </div>
            <div class="col-md-4 text-white">
                <h5>REDES INALÁMBRICAS</h5>
                <ul class="list-unstyled">
                    <li >Docente</li>
                    <li>MARLON PATIÑO BERNAL</li>
                    <li>marlonpb@udistrital.edu.co</li>
                </ul>
            </div>
            <div class="col-md-4 text-white">
                <h5>ESTUDIANTES</h5>
                <ul class="list-unstyled">
                    <li>Duvan Riaño</li>
                </ul>
            </div>
        </div>
    </div>
</footer>
